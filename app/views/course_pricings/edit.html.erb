
					 <div id="wrap">

  <%= render 'shared/side_menu' %>
              
            <div id="content">
               
                <div class="container-fluid outer"  >
                            <div class="row-fluid">
                                                        <div class="span12 inner">
                           
                            <div class="row-fluid">
                                <div class="span12">
                                    <div class="box">
 <div id="collapse4" class="body">
 	<div class="icons"><span class="badge badge-info">Edit Course Price</span></div>
  <table id="dataTable">
     <%= simple_form_for (@coursepricing) do |f| %>
			<%= render 'shared/error_messages', object: f.object %>
		<tr><th>Course Name</th><td>
				<%= f.select(:course_id, @course.collect {|p| [ p.title,p.id ] })%></td></tr>
				
				<tr><th>Start Date</th><td>
					<div >

						<%= f.input_field :start_date, :start_year => Date.today.year, :end_year => Date.today.year + 30, class:"date" %></td></tr>
					</td></tr></div>
					<tr><th>End Date</th><td>
						<%= f.input_field :end_date, :start_year => Date.today.year, :end_year => Date.today.year + 30, class:"date" %></td></tr>
						<tr><th>Price</th><td>
							<%= f.text_field :price %></td></tr>
							<tr><th></th><td><%= f.submit "Update" ,class: "btn btn-primary " %></td></tr>

							<% end %>
</table>                     
</div>
</div>
</div>
</div> 
</div>
 </div>
