                            <table id="dataTable">
                            <%= form_for @partner do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
<tr>
	<td><%=f.label 'Company Name'%></td>
	<td><%=f.text_field :company_name%></td>
</tr>

<tr>
	<td><%=f.label 'Logo' %></td>
	
	<td><%= f.file_field :partner %></td>
</tr>
<tr>
	<td><%=f.label 'Comments'%></td>
	<td><%=f.text_area :comments, :size => "30x10" %></td>
</tr>
<tr >
   <td><%= f.submit "Save" ,:class=>"btn btn-primary"%> </td>
   <td><%= link_to "Cancel", :back ,:class=>"btn btn-default"%> </td>
</tr>
<% end %>
