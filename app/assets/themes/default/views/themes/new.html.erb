 <div id="wrap">
  <%= render 'shared/side_menu' %>
   <div id="content" class="span9">
     <!-- .outer -->
     <div class="container-fluid outer"  >
       <div class="row-fluid">
         <div class="span12 inner">
           <!--Begin Datatables-->
           <div class="row-fluid">
             <div class="span12">
               <div class="box">
                 <div id="collapse4" class="body" style = "margin-right:500px">
                   <div class="icons"><span class="badge badge-info">New Themes </span></div>
                   <table id="dataTable" >
                     <%= form_for @themes do |f| %>
                         <%= render 'shared/error_messages', object: f.object %>
                         <tr>
                           <td><%=f.label 'Name'%></td>
                           <td><%=f.text_field :name%></td>
                         </tr>
                         <tr >
                           <td><%=  link_to "Cancel", :back,class:"btn"  %></td>
                           <td><%= f.submit "Save",class:"btn btn-primary yellobg" %> </td>
                         </tr>
                     <% end %>
                   </table>
                   <div class="left" style="padding: 0px;width: 16%;float: right;">
<a href="../account_themes" class="btn btn-default btn-large" style="margin:21px; margin-top:-126px;margin-left:100px;width: 140px;"><i class="icon icon-plus-sign"></i>Assign Theme</a>
                   </div>
                 </div>
               </div>
             </div>
           </div>
         </div>
       </div>
       <!-- /.outer -->
     </div>
   </div>
   <div id="push"></div>
   <div class="clearfix"></div>